{
  "name": "mdx-m3-viewer",
  "version": "5.13.0",
  "description": "A browser WebGL model viewer. Mainly focused on models of the games Warcraft 3 and Starcraft 2. Updated with latest Vite and React architecture.",
  "main": "dist/mjs/viewer.js",
  "types": "dist/cjs/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run build:vendors && npm run build:umd && npm run build:mjs && npm run build:clients",
    "build:umd": "vite build --mode=umd",
    "build:mjs": "vite build --mode=mjs",
    "build:clients": "npm run build:client:example && npm run build:client:downgrader && npm run build:client:map && npm run build:client:mdlx && npm run build:client:rebuild && npm run build:client:sanitytest && npm run build:client:tests && npm run build:client:weu && npm run build:client:mdlxoptimizer",
    "build:client:example": "vite build --mode=client-example",
    "build:client:downgrader": "vite build --mode=client-downgrader",
    "build:client:map": "vite build --mode=client-map",
    "build:client:mdlx": "vite build --mode=client-mdlx",
    "build:client:rebuild": "vite build --mode=client-rebuild",
    "build:client:sanitytest": "vite build --mode=client-sanitytest",
    "build:client:tests": "vite build --mode=client-tests",
    "build:client:weu": "vite build --mode=client-weu",
    "build:client:mdlxoptimizer": "vite build --mode=client-mdlxoptimizer",
    "build:vendor:events": "VITE_LIB_ENTRY=node_modules/.pnpm/events@3.3.0/node_modules/events/events.js VITE_LIB_NAME=EventEmitter VITE_FILE_NAME=events.vendor vite build --config vite.vendor.config.js",
    "build:vendor:fengari-web": "VITE_LIB_ENTRY=node_modules/.pnpm/fengari-web@0.1.4/node_modules/fengari-web/dist/fengari-web.js VITE_LIB_NAME=fengariWeb VITE_FILE_NAME=fengari-web.vendor vite build --config vite.vendor.config.js",
    "build:vendor:gl-matrix": "VITE_LIB_ENTRY=node_modules/.pnpm/gl-matrix@3.3.0/node_modules/gl-matrix/gl-matrix.js VITE_LIB_NAME=glMatrix VITE_FILE_NAME=gl-matrix.vendor vite build --config vite.vendor.config.js",
    "build:vendor:pako": "VITE_LIB_ENTRY=node_modules/.pnpm/pako@2.1.0/node_modules/pako/dist/pako.js VITE_LIB_NAME=pako VITE_FILE_NAME=pako.vendor vite build --config vite.vendor.config.js",
    "build:vendor:tga-js": "VITE_LIB_ENTRY=node_modules/.pnpm/tga-js@1.1.1/node_modules/tga-js/dist/umd/tga.js VITE_LIB_NAME=TGA VITE_FILE_NAME=tga-js.vendor vite build --config vite.vendor.config.js",
    "build:register:umd": "vite build --config vite.register.config.js --mode=umd",
    "build:register:mjs": "vite build --config vite.register.config.js --mode=mjs",
    "build:register:client": "vite build --config vite.register.config.js --mode=client",
    "build:register": "npm run build:register:umd && npm run build:register:mjs && npm run build:register:client",
    "build:vendors": "mkdir -p vendors && npm run build:vendor:events && npm run build:vendor:fengari-web && npm run build:vendor:gl-matrix && npm run build:vendor:pako && npm run build:vendor:tga-js && npm run build:register:client",
    "dev": "vite",
    "serve": "vite preview",
    "prepublishOnly": "node clean.cjs && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flowtsohg/mdx-m3-viewer.git"
  },
  "author": "Chananya Freiman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flowtsohg/mdx-m3-viewer/issues"
  },
  "homepage": "https://github.com/flowtsohg/mdx-m3-viewer#readme",
  "dependencies": {
    "events": "^3.3.0",
    "fengari": "^0.1.4",
    "fengari-web": "^0.1.4",
    "gl-matrix": "3.3.0",
    "mdx-m3-viewer": "link:",
    "pako": "^2.0.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tga-js": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.2",
    "@types/pako": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7"
  }
}